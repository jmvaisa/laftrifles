##############################################################################
# The allowable variation in the size of the first word will be the combined
# size minus twice the minimum size.
##############################################################################

VARSIZE=$(( COMBSIZE + 1 - $M2 ))

REPLY=""
until [ "$REPLY" = q ]
do
	S1=$(( (RANDOM % VARSIZE) + $MIN ))
	S2=$(( COMBSIZE - S1 ))

	WD=()
	for SIZE in $S1 $S2
	do
		WORD=$(echo "${WORDLIST}" | xxd -p -r |
		gunzip | grep "^.\{${SIZE}\}$" | shuf | head -1)
		WD=( "${WD[@]}" $WORD )
	done

	if [ "$DOCAPS" ]
	then
		echo "${WD[0]^}${SEP}${WD[1]^}"
	else
		echo "${WD[0]}${SEP}${WD[1]}"
	fi

	read -p "----> Press Enter for next, chars for a new separator string, or q to quit: "
	test "$REPLY" && SEP="$REPLY"
done

##############################################################################
# END
##############################################################################
